{
    "docker.languageserver.diagnostics.instructionCasing": "error",
    "clang-format.executable": "clang-format",
    "go.useLanguageServer": true,
    "go.testEnvFile": "./env",
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 64,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": true
    }
}